#include <iostream>
using namespace std;

int main() {
    // For loop: Print numbers from 1 to 5
    cout << "For loop:\n";
    for (int i = 1; i <= 5; i++) {
        cout << i << " ";
    }
    cout << endl;

    // While loop: Print numbers from 1 to 5
    cout << "While loop:\n";
    int j = 1;
    while (j <= 5) {
        cout << j << " ";
        j++;
    }
    cout << endl;

    // Do-while loop: Print numbers from 1 to 5
    cout << "Do-While loop:\n";
    int k = 1;
    do {
        cout << k << " ";
        k++;
    } while (k <= 5);
    cout << endl;

    return 0;
}
