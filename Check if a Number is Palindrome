#include <iostream>
using namespace std;

int main() {
    int num, reversed = 0, original, digit;
    cout << "Enter a number: ";
    cin >> num;
    original = num;

    while(num > 0) {
        digit = num % 10;
        reversed = reversed * 10 + digit;
        num /= 10;
    }

    if(original == reversed)
        cout << "Itâ€™s a palindrome, baby ğŸ’";
    else
        cout << "Nope, not a palindrome ğŸ˜¢";

    return 0;
}
