using System;

class Program
{
    static void Main()
    {
        int num = 59238;
        int smallest = 9;

        while (num > 0)
        {
            int digit = num % 10;
            if (digit < smallest)
                smallest = digit;
            num /= 10;
        }

        Console.WriteLine("Smallest Digit = " + smallest);
    }
}
