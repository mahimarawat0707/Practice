using System;

class Program
{
    static void Main()
    {
        int num = 59238;
        int largest = 0;

        while (num > 0)
        {
            int digit = num % 10;
            if (digit > largest)
                largest = digit;
            num /= 10;
        }

        Console.WriteLine("Largest Digit = " + largest);
    }
}
